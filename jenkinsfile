pipeline {
    agent any
     
    stages {
      stage('checkout') {
           steps {
             
                git 'https://github.com/sruthiyelti/game-of-life.git'
          }
        }
        stage('code'){
            steps {
                withSonarQubeEnv(credentialsId: 'sonartoken', installationName: 'sonarqube') {
                       sh 'mvn package sonar:sonar'  
   } 
        
   }
       }
           }
}
